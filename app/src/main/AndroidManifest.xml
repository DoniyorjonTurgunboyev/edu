<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <application
        android:name=".app.App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:enableOnBackInvokedCallback="true"
        android:theme="@style/Theme.Edu"
        android:usesCleartextTraffic="true"
        tools:targetApi="tiramisu">
        <activity android:name=".iu.FinalMainActivity"/>
        <activity
            android:name=".iu.MainActivity"
            android:exported="true">
        </activity>
        <activity android:name="uz.smartarena.edu.iu.RegistrationActivity" />
        <activity android:name="uz.smartarena.edu.iu.VerifyActivity" />
        <activity android:name="uz.smartarena.edu.iu.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>